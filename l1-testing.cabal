  build-depends:       base >=4.7 && <4.8
                     , casadi-bindings
                     , dynobud
                     , generic-accessors >= 0.1.0.1
                     , hmatrix
                     , hmatrix-gsl
                     , linear
                     , vector
  hs-source-dirs:      src
  default-language:    Haskell2010

executable runme
  hs-source-dirs:      apps
  main-is:             L1Test.hs
  build-depends:       base >=4.7 && <4.8
                       , l1-testing
                       , casadi-bindings
                       , dynobud
                       , generic-accessors >= 0.1.0.1
                       , hmatrix
                       , hmatrix-gsl
                       , linear
                       , vector
  default-language:    Haskell2010
  ghc-options:         -O2 -Wall
